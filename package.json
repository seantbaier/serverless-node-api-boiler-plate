    "aws-sdk": "^2.773.0",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^2.0.0",
    "express-paginate": "^1.0.2",
    "express-rate-limit": "^5.1.3",
    "generate-password": "^1.5.1",
    "helmet": "^4.1.1",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.4.0",
